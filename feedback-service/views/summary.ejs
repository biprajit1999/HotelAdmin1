<!DOCTYPE html>
<html lang="en">
    <script src="https://unpkg.com/ejs/ejs.min.js"></script>
    <div id="navbar"></div>
    <script>
        fetch('/ds/navbar') 
        .then(response => response.text())
        .then(html => {
            document.getElementById('navbar').innerHTML = ejs.render(html);
        })
        .catch(error => {
            console.error('Error fetching the navbar:', error);
        });
    </script>
<head>
   
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" type="text/css" href="/dsb/css/style.css">
</head>
<body>
    <h1 style="text-align: center;">Paradise Cove Retreat Analytics Dashboard</h1>
	<iframe style="background: #21313C;border: none;border-radius: 2px;box-shadow: 0 2px 10px 0 rgba(70, 76, 79, .2);width: 100vw;height: 100vh;"  src="https://charts.mongodb.com/charts-project-0-xifan/embed/dashboards?id=d6ef23c7-4cec-4c3a-8362-34f71a8ec085&theme=dark&autoRefresh=true&maxDataAge=60&showTitleAndDesc=true&scalingWidth=scale&scalingHeight=scale"></iframe>
</body>

<div id="footer" style="position: fixed; left: 0; bottom: 0; width: 100%;"></div>
	<script>
		fetch('/ds/footer') 
        .then(response => response.text())
        .then(html => {
            document.getElementById('footer').innerHTML = ejs.render(html);
        })
        .catch(error => {
            console.error('Error fetching the footer:', error);
        });
	</script>
</html>